{
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "dist",
        "declaration": true
    },
    "include": ["src"]
}
